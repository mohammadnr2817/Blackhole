{
    "manifest_version": 3,
    "name": "Blackhole",
    "description": "Redirect forbidden websites.",
    "version": "0.1",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }